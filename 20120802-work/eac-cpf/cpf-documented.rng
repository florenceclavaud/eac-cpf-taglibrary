               </optional>
               <optional>
                  <ref name="lang"/>
               </optional>
               <ref name="a.localType"/>
               <ref name="m.simpleDescription"/>
            </element>
         </define>
         <define name="e.languageUsed">
            <element a:id="languageUsed" name="languageUsed">
               <optional>
